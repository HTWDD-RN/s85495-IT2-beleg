#!/usr/local/bin/gnuplot --persist
set title "Image Loss Probability vs Channel Loss Rate"
set xlabel "Channel Loss Rate (P_e)"
set ylabel "Image Loss Probability"
set grid
set key left top
plot "Fec_result.txt" using 1:2 with lines title "k=2", \
     "Fec_result.txt" using 1:3 with lines title "k=6", \
     "Fec_result.txt" using 1:4 with lines title "k=12", \
     "Fec_result.txt" using 1:5 with lines title "k=48"
pause -1